package game;

public class Postre extends Productos{

	String ingredientes;
	int tiempoPreparacion;
	
	public Postre(String receta, int precio, String nombre, int cantidad, String ingredientes, int tiempoPreparacion) {
		super(receta, precio, nombre, cantidad);
		this.ingredientes = ingredientes;
		this.tiempoPreparacion = tiempoPreparacion;
	}
	
}
