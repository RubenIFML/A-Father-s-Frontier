package game;

public class Tienda {
	
	static Productos producto;
	static Postre postre;
	
	public static void main(String[] args) {
		
		producto = new Productos("Harina, Levadura, Huevos", 3, "Bizcocho", 1);
		postre = new Postre("Harina, Levadura, Huevos", 3, "Bizcocho", 1, "Aceite", 30);

		producto.toString();
		postre.toString();
		
	}
}
